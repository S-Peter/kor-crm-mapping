<h1>Mapping Results</h1>
<br />
<h2>KorKind-Mapping</h2>
<table>
	<tr>
		<th>KorKind</th>
		<th>CRMClass</th>
	</tr>
	<% @kinds.each do |kind| %>
    <tr>
      <td><%= kind.name %></td>
      <td><%= kind.crmClass.label %></td>
    </tr>
  <% end %>
</table>
<br/>
<h2>KorRelation-Mapping</h2>
<table>
	<tr>
		<th>KorRelation</th>
		<th>KorDomain</th>
		<th>KorRange</th>
		<th>CRMPath</th>
	</tr>
	<% @relations.each do |relation| %>
		<% relation.actualRelations.each do |actualRelation| %>
    		<tr>
		      	<td><%= relation.name %></td>
		      	<td><%= actualRelation.domain.name %></td>
		      	<td><%= actualRelation.range.name %></td>
		      	<td>
		      		<% actualRelation.chainLinks.each do |crmRessource| %>
		      			<%= crmRessource.label %>
		      			<%= " " %>
		      		<% end %>
		      	</td>
    		</tr>
    	<% end %>
  	<% end %>
</table>
